wave create -driver freeze -pattern constant -value 1 -starttime 0ps -endtime 1000ps sim:/ula/ALUop
wave create -driver freeze -pattern clock -initialvalue 0 -period 200ps -dutycycle 50 -starttime 0ps -endtime 1000ps sim:/ula/clk
add wave -position end  sim:/ula/Result
add wave -position end  sim:/ula/Zero
add wave -position end  sim:/ula/Overflow
