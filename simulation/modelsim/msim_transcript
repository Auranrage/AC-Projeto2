vsim work.reg_instrucao
# vsim work.reg_instrucao 
# Start time: 21:05:09 on Oct 29,2021
# Loading std.standard
# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading work.reg_instrucao(behavior)
add wave -position end  sim:/reg_instrucao/OP
add wave -position end  sim:/reg_instrucao/RS
add wave -position end  sim:/reg_instrucao/RT
add wave -position end  sim:/reg_instrucao/RD
wave create -driver freeze -pattern clock -initialvalue 0 -period 1000ps -dutycycle 50 -starttime 0ps -endtime 10000ps sim:/reg_instrucao/Clock
# Editable Wave items are not compatible with the wave window's standard undo-redo.  Standard undo-redo has been disabled.
wave create -driver freeze -pattern constant -value 1 -starttime 0ps -endtime 10000ps sim:/reg_instrucao/load
wave create -driver freeze -pattern constant -value 00101101 -range 7 0 -starttime 0ps -endtime 1000ps sim:/reg_instrucao/instrucao
wave modify -driver freeze -pattern constant -value 11000010 -range 7 0 -starttime 0ps -endtime 1000ps Edit:/reg_instrucao/instrucao
wave modify -driver freeze -pattern constant -value 00000101 -range 7 0 -starttime 1000ps -endtime 2000ps Edit:/reg_instrucao/instrucao
run
